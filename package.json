{
    "name": "vscode-ydsh",
    "displayName": "vscode-ydsh",
    "description": "vscode extension for ydsh",
    "version": "0.0.3",
    "license": "Apache-2.0",
    "homepage": "https://github.com/sekiguchi-nagisa/vscode-ydsh/blob/master/README.md",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "publisher": "nagisa-sekiguchi",
    "repository": {
        "type": "git",
        "url": "https://github.com/sekiguchi-nagisa/vscode-ydsh.git"
    },
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:ydsh"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ydsh",
                "aliases": [
                    "ydsh",
                    "ydsh"
                ],
                "extensions": [
                    "ds"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ydsh",
                "scopeName": "source.ydsh",
                "path": "./syntaxes/ydsh.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "@types/mocha": "^8.2.2",
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "eslint": "^7.23.0",
        "mocha": "^8.3.2",
        "typescript": "^4.3.2",
        "vsce": "^1.88.0",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "vscode-languageclient": "^7.0.0"
    },
    "private": true,
    "scripts": {
        "compile": "tsc -b",
        "build": "vsce package"
    }
}
